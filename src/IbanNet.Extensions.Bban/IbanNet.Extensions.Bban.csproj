<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard1.2;netstandard1.6;netstandard2.0;net47;net45</TargetFrameworks>
    <RootNamespace>IbanNet</RootNamespace>

    <Description>IbanNet BBAN validation extensions.</Description>
    <PackageProjectUrl>https://github.com/skwasjer/IbanNet.Extensions.Bban</PackageProjectUrl>
    <PackageTags>BBAN, Validation, IBAN, International Bank Account Number</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <!-- For time being, use floating version, so we always pull latest from feed. -->
    <PackageReference Include="IbanNet" Version="3.2.0-feature-extensibility.build.193-alpha-004" />
  </ItemGroup>

</Project>
